@model IEnumerable<ImdbLite.Web.Areas.Administration.ViewModels.Articles.ArticleIndexViewModel>
@{
    ViewBag.Title = "Articles";
}
@Html.Partial("_ModalDialogPartial")
<div class="row">
    <h2>@ViewBag.Title</h2>
</div>
<div class="row">
    @Html.ActionLink("New Article", "Create", "Articles", new { area = GlobalConstants.AdminArea }, new { @class = "btn btn-default" })
</div>
<hr>
<div class="row" id="update-target">
    @Html.Action("ReadData", "Articles", new { area = GlobalConstants.AdminArea })
</div>
@section scripts{
    @Scripts.Render("~/bundles/scroll-to-top")
}
